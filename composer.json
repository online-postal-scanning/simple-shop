{
  "name": "olps/simple-shop",
  "type": "library",
  "description": "A small feature set ecommerce library",
  "keywords": ["ecommerce"],
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "doctrine/dbal": "^2.12.1 || ^3.0",
    "iampersistent/get-off-my-case": "^1.2.0",
    "olps/money-helpers": "^v1.0.0",
    "moneyphp/money": "^v4.0.5",
    "payum/core": "^1.7.5",
    "robmorgan/phinx": "^0.15 || ^0.16",
    "php-http/message-factory": "^1.1"
  },
  "require-dev": {
    "php-http/guzzle7-adapter": "^1.0",
    "phpunit/phpunit": "^11",
    "symfony/yaml": "^6.0 || ^7.0",
    "payum/offline": "^1.3.7",
    "symfony/dotenv": " ^6.4|^7.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "OLPS\\SimpleShop\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Integration\\": "tests/Integration",
      "Tests\\Unit\\": "tests/Unit"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
