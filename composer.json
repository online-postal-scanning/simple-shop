{
  "name": "olps/simple-shop",
  "type": "library",
  "description": "A small feature set ecommerce library",
  "keywords": ["ecommerce"],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "doctrine/dbal": "^2.12.1 | ^3",
    "iampersistent/get-off-my-case": "^1.2.0",
    "olps/money-helpers": "^v1.0.0",
    "moneyphp/money": "^v4.0.5",
    "payum/core": "^1.7.2",
    "php-http/guzzle7-adapter": "^1.0",
    "php-http/message-factory": "^1.1",
    "psr/container": "^1.0 || ^2.0",
    "robmorgan/phinx": "^0.16"
  },
  "require-dev": {
    "payum/offline": "~1.7",
    "phpunit/phpunit": "^11",
    "symfony/yaml": "^6.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "OLPS\\SimpleShop\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Integration\\OLPS\\SimpleShop\\": "tests/Integration",
      "Tests\\Unit\\OLPS\\SimpleShop\\": "tests/Unit"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "pestphp/pest-plugin": true
    }
  }
}
